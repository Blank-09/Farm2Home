{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "prettier",
    "plugin:react/recommended"
  ],
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "ecmaVersion": 12,
    "requireConfigFile": false,
    "sourceType": "module",
    "babelOptions": {
      "presets": [
        "@babel/preset-react"
      ]
    }
  },
  "plugins": [
    "react-hooks"
  ],
  "rules": {},
  "settings": {
    "react": {
      "version": "17.0"
    }
  }
}